CONFIG_SYS_CONFIG_NAME="agc032a"
CONFIG_DEFAULT_DEVICE_TREE="agc032a"
CONFIG_FBOBMC_IMG_LAYOUT_VER=1
CONFIG_ASPEED_TPM=n

# DELTA
CONFIG_BAUDRATE=115200
CONFIG_SYS_TEXT_BASE=0x0
CONFIG_SPI_BOOT=y
CONFIG_HUSH_PARSER=y
#CONFIG_NETCONSOLE=y
CONFIG_NETDEVICES=y
CONFIG_DEBUG_UART=y
CONFIG_DEBUG_UART_BASE=0x1E784000
CONFIG_DEBUG_UART_CLOCK=24000000
CONFIG_BOOTDELAY=5
CONFIG_SYS_PROMPT="agc032a-boot=> "
CONFIG_ASPEED_ENABLE_DUAL_BOOT_WATCHDOG=y

# Debug
CONFIG_LOGLEVEL=7
